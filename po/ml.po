#: ../src/iso2022.c:784 ../src/iso2022.c:795 ../src/iso2022.c:844
#: ../src/vte.c:1552
#, c-format
msgid "Unable to convert characters from %s to %s."
msgstr "%s-ല്‍ നിന്നും %s-ലേക്ക് അക്ഷരങ്ങളെ വേര്‍തിരിക്കുവാന്‍ സാധിക്കുന്നില്ല."

#: ../src/iso2022.c:1574
#, c-format
msgid "Attempt to set invalid NRC map '%c'."
msgstr "അസാധുവായ NRC മാപ്പ് '%c' ക്രിമീകരിക്കുവാനുളള ശ്രമം."

#. Application signalled an "identified coding system" we haven't heard of.  See ECMA-35 for gory details.
#: ../src/iso2022.c:1615
msgid "Unrecognized identified coding system."
msgstr "അംഗീകാരമില്ലാത്ത ഐഡന്‍റിഫൈഡ് കോഡിങ് സിസ്റ്റം."

#: ../src/iso2022.c:1664 ../src/iso2022.c:1690
#, c-format
msgid "Attempt to set invalid wide NRC map '%c'."
msgstr "അസാധുവായ വീതിയുളള NRC മാപ്പ് '%c' ക്രിമീകരിക്കുവാനുളള ശ്രമം."

#: ../src/pty.c:331
#, c-format
msgid "Error adding `%s' to environment, continuing."
msgstr "%s നെ ചുറ്റുപാടുമായി ചേര്‍ക്കുന്നതില്‍ പിശക്, എങ്കിലും മുന്നോട്ട് നീങ്ങുന്നു."

#. Give the user some clue as to why session logging is not
#. * going to work (assuming we can open a pty using some other
#. * method).
#: ../src/pty.c:916
#, c-format
msgid "can not run %s"
msgstr "%s പ്രവര്‍ത്തിപ്പിക്കുവാന്‍ കഴിയുന്നില്ല"

#: ../src/reaper.c:147
msgid "Error creating signal pipe."
msgstr "സിഗ്നല്‍ പൈപ്പ് ഉണ്ടാക്കുന്നതില്‍ പിഴവ്."

#: ../src/trie.c:414
#, c-format
msgid "Duplicate (%s/%s)!"
msgstr "തനിപകര്‍പ്പ് (%s/%s)!"

#: ../src/vte.c:971
#, c-format
msgid "Error compiling regular expression \"%s\"."
msgstr "ക്രമത്തിലുളള പ്രയോഗം \"%s\"സമാഹാരിക്കുന്നതില്‍ പിശക്."

#: ../src/vte.c:2434
#, c-format
msgid "No handler for control sequence `%s' defined."
msgstr "control sequence `%s'-ന് ഹാന്‍ഡ്ലറുകള്‍ ലഭ്യമല്ല."

#. Translators: %s is replaced with error message returned by strerror().
#: ../src/vte.c:3282
#, c-format
msgid "Error reading from child: %s."
msgstr "ചൈള്‍ഡില്‍ നിന്നും വായിക്കുന്നതില്‍ പിശക്: %s."

#: ../src/vte.c:3404 ../src/vte.c:4341
#, c-format
msgid "Error (%s) converting data for child, dropping."
msgstr "ചൈള്‍ഡിന് ഡേറ്റാ വേര്‍തിരിക്കുന്നതില്‍ പിഴവ് (%s), ഉപേക്ഷിക്കുന്നു."

#: ../src/vte.c:6304
#, c-format
msgid "Error reading PTY size, using defaults: %s."
msgstr "PTY വലിപ്പം വായിക്കുന്നതില്‍ പിശക്, ഡീഫോള്‍ട്ടുകള്‍ ഉപയോഗിക്കുന്നു: %s."

#: ../src/vte.c:6340
#, c-format
msgid "Error setting PTY size: %s."
msgstr "PTY വലിപ്പം സജ്ജീകരിക്കുന്നതില്‍ തകരാര്‍: %s."

#. Aaargh.  We're screwed.
#: ../src/vte.c:10604
msgid "_vte_conv_open() failed setting word characters"
msgstr "_vte_conv_open() വാക്യത്തിലെ അക്ഷരങ്ങള്‍ സജ്ജീകരിക്കുന്നതില്‍ തകരാര്‍"

#. Bail back to normal mode.
#: ../src/vteapp.c:738
msgid "Could not open console.\n"
msgstr "കണ്‍സോള്‍ തുറക്കാനാവുന്നില്ലി\n"

#: ../src/vteglyph.c:578
#, c-format
msgid "Unknown pixel mode %d.\n"
msgstr "അറിയാത്ത പിക്സല്‍ രീതി %d.\n"

#: ../src/vtexft.c:699
#, c-format
msgid "Can not draw character U+%04x.\n"
msgstr "എഴുതാനാവാത്ത അക്ഷരം  U+%04x.\n"

